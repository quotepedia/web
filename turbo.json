{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "persistent": true
    },
    "lint": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
