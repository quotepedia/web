{
  "name": "@quotepedia/solid",
  "version": "0.1.0",
  "private": true,
  "description": "SolidJS components for Quotepedia",
  "keywords": [
    "solid",
    "solidjs"
  ],
  "homepage": "https://github.com/quotepedia/web",
  "bugs": {
    "url": "https://github.com/quotepedia/web/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quotepedia/web.git"
  },
  "license": "MIT",
  "author": {
    "name": "Nikita Gaziev",
    "email": "zobweyt@gmail.com",
    "url": "https://github.com/zobweyt"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "solid": "./dist/index.jsx",
      "default": "./dist/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "solid": "./dist/components/*/index.jsx",
      "default": "./dist/components/*/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo dist node_modules",
    "dev": "vite serve playground --host",
    "lint": "tsc"
  },
  "dependencies": {
    "@corvu/utils": "^0.4.2",
    "@kobalte/core": "^0.13.7",
    "@solid-primitives/event-listener": "^2.3.3",
    "@solid-primitives/intersection-observer": "^2.1.6",
    "clsx": "^2.1.1",
    "corvu": "^0.7.1",
    "lottie-web": "^5.12.2",
    "solid-persistent": "^0.1.0",
    "solid-transition-group": "^0.2.3",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0"
  },
  "devDependencies": {
    "@iconify-icon/solid": "^2.2.0",
    "@modular-forms/solid": "^0.25.0",
    "@quotepedia/tailwindcss": "workspace:^",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "solid-js": "^1.9.3",
    "tailwindcss": "^3.4.17",
    "valibot": "1.0.0-beta.9",
    "vite": "^6.0.5",
    "vite-plugin-solid": "^2.11.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "solid-js": "^1.9.3"
  }
}
